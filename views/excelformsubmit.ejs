<form action="/excelprojectso" method="POST">

<label for="projectName">Project Name: </label><br>
<input type="text" id="prj-name" name="projectName" size="24"><br><br>

<input type="hidden" id="createdBy" name="createdBy" value="<%=currentLoggedInUser%>">


<div class="abc">
    <div class="def">
        <div class ="neat">
            <label for="mfgPn">Mfg p/n:</label>
