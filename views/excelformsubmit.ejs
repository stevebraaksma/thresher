        <div class ="nifty">
            <label for="partQty">Qty</label>
        </div>
        <div class ="nerfty">
            <label for="internalPn">Internal p/n:</label>
        </div>
    </div>
</div>


<% for (let i = 1; i < 11; i++) { %>   

    <div class="abc">
        <div class="def">
            <div class="neat">
                <input type="text" name="bom[mfgPnBom]" value="<%=project[i][0]%>">
                
            </div>
            <div class="nifty">
                <input type="text" name="bom[partQtyBom]" value="<%=project[i][1]%>">
            </div>
            <div class="nerfty">
                <input type="text" name="bom[internalPnBom]" value="<%=project[i][2]%>">
            </div>
        </div>
    </div>


    <% }  %>



<br>
<input type="submit" value="Submit">


</form>

    
